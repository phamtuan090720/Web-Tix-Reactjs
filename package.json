    "web-vitals": "^1.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "watch:sass": "nodemon -e scss -x 'npm run compile:sass'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "compilerOptions": {
    "target": "ES6",
    "module": "esnext",
    "baseUrl": "./src",
    "paths": {
      "routes/*": [
        "./src/routes"
      ],
      "containers/*": [
        "./src/containers"
      ],
      "components/*": [
        "./src/components"
      ],
      "img/*": [
        "./src/img"
      ]
    }
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "exclude": [
    "node_modules",
    "build",
    "coverage",
    "dist",
    "lib"
  ]
}
